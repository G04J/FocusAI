      "!backend/test/**",
      "!**/node_modules/**"
    ]
  },
  "devDependencies": {
    "electron": "^39.2.7",
    "jest": "^29.7.0"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.5.0",
    "electron-store": "^11.0.2",
    "jsonwebtoken": "^9.0.3",
    "screenshot-desktop": "^1.15.3"
  }
}
